{
  "manifest_version": 2,
  "name": "achievibit",
  "version": "1.1.11",
  "description": "get achievements while coding on GitHub",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "https://achievibit.herokuapp.com/"
  ],
  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "jquery-3.1.1.min.js",
        "socket.io.min.js",
        "content.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ]
}
